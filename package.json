{
  "name": "authenticator-lib",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "npx ts-node-dev src/index.ts",
    "start": "npm run build && node dist/src/index.js",
    "start-container": "npm run build && docker-compose up",
    "test": "npm run test:db:init && jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
    "test:verbose": "jest --passWithNoTests --watch --runInBand",
    "test:unit": "npm run test -- --watch -c jest-unit-config.js",
    "test:integration": "npm run test:db:init && jest --passWithNoTests --runInBand -c jest-integration-config.js",
    "test:staged": "npm run test -- --findRelatedTests",
    "test:coverage": "npm run test:db:init && jest --passWithNoTests --silent --noStackTrace --no-cache --coverage",
    "test:db:init": "sequelize db:migrate:undo:all --env test && sequelize db:migrate --env test"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "jest": "^27.5.1",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.17.3",
    "nodemailer": "^6.9.13",
    "sequelize": "^6.37.2",
    "sqlite3": "^5.1.7",
    "zod": "^3.22.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
